model{
  # proportion strata
  for (z in 1:N.z){
    p.z[z] ~ dbeta(n.z[z]*rho.z[z], n.z[z]*(1-rho.z[z]))
    rho.z[z] <- min(1-eps,max(eps,
      mean((x.i > x.z[z]) && (x.i <= x.z[z+1]))))
  }
  # respondents
  for (i in 1:N.i){
    Q.i[i] ~ dgamma(Q.shape,Q.rate)
    x.i[i] ~ dpois(Q.i[i]*recall.eff)
  }
  # globals
  Q.shape ~ dunif(0,1e3)
  Q.rate ~ dunif(0,1e3)
  x <- mean(x.i)
  Q <- Q.shape / Q.rate
  K <- Q * dur
}
